%!PS-Adobe-2.0 EPSF-2.0
%%BoundingBox: 0 0 240 120
%%Creator: Mark Calabretta
%%CreationDate: Sun 1992/09/20
%%Title: Hammer-Aitoff projection.
%%Pages: 1 1
%%DocumentFonts: Courier
%%EndComments

%%BeginProcSet: graphics.pro
   /TextFont /Courier-Bold findfont 8 scalefont def

   % Constants.
   /pi 3.1416 def
   /ws 0.2 def

   % Radius of the generating sphere.
   /r0 40 def

   /DrawMeridian {
      ws mul setlinewidth
      2 div dup
      cos /a1 exch def
      sin /a2 exch def
      /theta 90 def
      0 2 sqrt r0 mul moveto
      36 {
         /theta theta 5 sub def
         /a0 theta cos a1 mul 1 add 2 div sqrt def
         -2 r0 mul theta cos mul a2 mul a0 div
         r0 theta sin mul a0 div lineto
         } repeat
      stroke
      -2 r0 mul a2 mul a1 1 add 2 div sqrt div 0 moveto
      -2 2 rmoveto
      90 rotate
      show
      -90 rotate
      stroke
      } def

   /DrawParallel {
      ws mul setlinewidth
      dup
      cos /a1 exch def
      sin /a2 exch def
      /phi -180 def
      2 sqrt r0 mul dup 2 mul a1 mul exch a2 mul moveto
      72 {
         /phi phi 5 add def
         /a0 phi 2 div cos a1 mul 1 add 2 div sqrt def
         -2 r0 mul a1 mul phi 2 div sin mul a0 div
         r0 a2 mul a0 div lineto
         } repeat
      0 r0 a2 mul a1 1 add 2 div sqrt div moveto
      1 2 rmoveto
      show
      stroke
      } def

%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

125 60 translate

  (0)    0 4 DrawMeridian
   ()   15 2 DrawMeridian
 (30)   30 3 DrawMeridian
   ()   45 2 DrawMeridian
 (60)   60 3 DrawMeridian
   ()   75 2 DrawMeridian
 (90)   90 4 DrawMeridian
   ()  105 2 DrawMeridian
(120)  120 3 DrawMeridian
   ()  135 2 DrawMeridian
(150)  150 3 DrawMeridian
   ()  165 2 DrawMeridian
   ()  180 4 DrawMeridian
(180) -180 4 DrawMeridian
   () -165 2 DrawMeridian
(210) -150 3 DrawMeridian
   () -135 2 DrawMeridian
(240) -120 3 DrawMeridian
   () -105 2 DrawMeridian
(270)  -90 4 DrawMeridian
   ()  -75 2 DrawMeridian
(300)  -60 3 DrawMeridian
   ()  -45 2 DrawMeridian
(330)  -30 3 DrawMeridian
   ()  -15 2 DrawMeridian

   ()   75 2 DrawParallel
 (60)   60 3 DrawParallel
   ()   45 2 DrawParallel
 (30)   30 3 DrawParallel
   ()   15 2 DrawParallel
  (0)    0 4 DrawParallel
   ()  -15 2 DrawParallel
(-30)  -30 3 DrawParallel
   ()  -45 2 DrawParallel
(-60)  -60 3 DrawParallel
   ()  -75 2 DrawParallel

vmsave restore
showpage


%%Trailer
userdict /end-hook known {end-hook} if
%%EOF
